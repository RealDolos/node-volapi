{
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": ["lib"]
  },
  "sourceType": "package",
  "opts": {
    "destination": "./docs/",
    "readme": "Readme.md",
    "template": "node_modules/docdash",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "outputSourceFiles": false,
    "includeDate": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false
    }
  }
}
